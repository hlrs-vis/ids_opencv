#!/bin/bash

[ "$0" != "$BASH_SOURCE" ] && SOURCED=1 || SOURCED=0
export ${LIBRARY_NAME_UPPER_VISION_API}_PATH=$(readlink -f "$(dirname "$BASH_SOURCE")")
export LD_LIBRARY_PATH=${${LIBRARY_NAME_UPPER_VISION_API}_LIBRARY_DIR}/../:$LD_LIBRARY_PATH
export GENICAM_GENTL32_PATH=$GENICAM_GENTL32_PATH:${${LIBRARY_NAME_UPPER_VISION_API}_LIBRARY_DIR}/../ids/cti
export GENICAM_GENTL64_PATH=$GENICAM_GENTL64_PATH:${${LIBRARY_NAME_UPPER_VISION_API}_LIBRARY_DIR}/../ids/cti

[ "$SOURCED" -eq 0 ] && $${LIBRARY_NAME_UPPER_VISION_API}_PATH/${VSSL_SAMPLE_BINARY_NAME} $@
